/* SCROLL*/
function scrollTop(){
	if ((document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop) >= 200) doScrollTop();
}

function doScrollTop(pixels) {
	var scrollY = (document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop);
	var scrollX = (document.documentElement.scrollLeft ? document.documentElement.scrollLeft : document.body.scrollLeft);
	if (pixels == null) pixels = scrollY / 10;
	if (scrollY - pixels > 0) {
		window.scrollTo(scrollX, scrollY - pixels);
		setTimeout('doScrollTop('+pixels+')', 40);
	}
	else window.scrollTo(scrollX, 0);
}

function scrollBottom() {
	var browser = getBrowser();
	if (browser.ns) scrollBottomMoz();

	document.attachEvent('onmousedown', new Function('document.getElementById("'+menuId+'").style.display="none"; document.detachEvent("onmousedown", arguments.callee);'));

}

function scrollBottomMoz(pixels) {
	var scrollY = window.pageYOffset;
	var scrollX = window.pageXOffset;
	var height = window.innerHeight;
	
	if (pixels == null) pixels = (height-scrollY) / 10;
	window.scrollTo(scrollX, scrollY + pixels);
	scrollYNew = window.pageYOffset;
	if (scrollYNew - scrollY == pixels) {
		setTimeout('scrollBottomMoz('+pixels+')',40);
	}
}

